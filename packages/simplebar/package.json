{
  "version": "4.0.0-stable",
  "name": "@bitsler/simplebar",
  "title": "SimpleBar.js",
  "description": "Scrollbars, simpler.",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Adrien Denat from a fork by Jonathan Nicol",
  "repository": "uniibu/simplebar",
  "main": "dist/simplebar.min.js",
  "module": "dist/simplebar.esm.js",
  "style": "dist/simplebar.min.css",
  "homepage": "https://github.com/uniibu/simplebar/",
  "bugs": "https://github.com/uniibu/simplebar/issues",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --mode=development",
    "clean": "rm -rf ./dist",
    "build": "yarn clean & rollup -c && cp src/simplebar.css dist/simplebar.css && minify dist/simplebar.css > dist/simplebar.min.css && webpack --mode=production",
    "dev": "rollup -c -w",
    "test": "yarn test:unit && yarn test:e2e",
    "test:unit": "jest -c jest-unit.config.js",
    "test:e2e": "jest -c jest-e2e.config.js"
  },
  "dependencies": {
    "can-use-dom": "^0.1.0",
    "core-js": "^3.0.1",
    "lodash.debounce": "^4.0.8",
    "lodash.memoize": "^4.1.2",
    "lodash.throttle": "^4.1.1",
    "resize-observer-polyfill": "^1.5.1",
    "scrollbarwidth": "^0.1.3"
  },
  "devDependencies": {
    "css-loader": "^2.1.1",
    "minify": "^4.1.1",
    "promise": "^8.0.3",
    "react-select": "^1.3.0",
    "style-loader": "^0.23.1"
  },
  "gitHead": "154e640a58a4c36eaf7aaa0ad4e4925efdc5fffd"
}
